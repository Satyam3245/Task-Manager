generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  todos Todos[]
}

model Todos {
  id String @id @default(uuid())
  task String
  Done Boolean @default(false)
  user User @relation(fields: [userId],references: [id])
  userId String
}